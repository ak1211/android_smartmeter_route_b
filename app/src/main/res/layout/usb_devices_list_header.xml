<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/usb_devices_list_header_label1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="@string/usb_devices_list_header_label1"
        android:textSize="14sp"
        app:layout_constraintBaseline_toBaselineOf="@id/usb_devices_list_header_num_of_items_text"
        app:layout_constraintEnd_toStartOf="@+id/usb_devices_list_header_num_of_items_text" />

    <TextView
        android:id="@+id/usb_devices_list_header_num_of_items_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="0"
        android:textSize="34sp"
        app:layout_constraintEnd_toStartOf="@+id/usb_devices_list_header_label2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/usb_devices_list_header_label2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="@string/usb_devices_list_header_label2"
        android:textSize="14sp"
        app:layout_constraintBaseline_toBaselineOf="@id/usb_devices_list_header_num_of_items_text"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>




